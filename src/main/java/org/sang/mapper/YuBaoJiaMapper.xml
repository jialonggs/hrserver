    <insert id="addYuBaoJia" useGeneratedKeys="true" keyProperty="id" parameterType="org.sang.bean.YuBaoJia">
        INSERT INTO work_yubaojia set projectId=#{projectId},auditId=#{auditId},clientCompanyName=#{clientCompanyName},contactPerson=#{contactPerson},contactPhone=#{contactPhone},carType=#{carType},tax=#{tax},
        audit=#{audit},total=#{total},taxTotal=#{taxTotal},expectTime=#{expectTime},addUserId=#{addUserId},addUserName=#{addUserName},
        status=#{status},remark=#{remark},isDeleted=FALSE,immutable=FALSE,updateTime=now(),createTime=now()
    </insert>

    <select id="getYuBaoJiaList" resultType="org.sang.bean.YuBaoJia">
        SELECT * FROM work_yubaojia WHERE isDeleted=FALSE AND addUserId=#{addUserId} ORDER BY id
